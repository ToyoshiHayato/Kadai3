<%= render 'list',users: @user %>


  <%= link_to "Edit", edit_user_path(@user) %>
  
  
 
  

<h2>New book</h2>
<%= form_with model: @book_new do |f| %>
<h6>Title</h6>
<%= f.text_field :title %>
<h6>Opinion</h6>
<%= f.text_area :body %><br>
<%= f.submit 'Create Book' %>
<% end %>
<h2>Users</h2>
<tr>
  <th>image</th>
  <th>name</th>
</tr><br>
<% @users.each do |user| %>
<%= image_tag user.get_profile_image %>
<tr>
    <th><%= user.name %></th>
<%= link_to user_path(user.id) do %>
    <th>Show</th>
</tr><br>
  <% end %>
<% end %>